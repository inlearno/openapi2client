{
  "name": "openapi2client",
  "version": "1.0.3-0",
  "description": "Transform Inlearno OpenApi v3 schemes into typescript client + redux actions",
  "main": "cli.js",
  "scripts": {
    "test": "jest",
    "build": "babel src -d dist",
    "changelog": "conventional-changelog-writer -p angular -i CHANGELOG.md -w -r 0"
  },
  "bin": {
    "openapi-client-generator": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inlearno/openapi2client.git"
  },
  "keywords": [
    "openapi",
    "inlearno"
  ],
  "author": "Ivan Chirkin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inlearno/openapi2client/issues"
  },
  "homepage": "https://github.com/inlearno/openapi2client#readme",
  "dependencies": {
    "@typescript-eslint/parser": "^2.16.0",
    "case": "^1.6.2",
    "dotenv": "^8.2.0",
    "meow": "^6.0.0",
    "node-fetch": "^2.6.0",
    "prettier-eslint": "^9.0.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "babel-jest": "^24.9.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "coverageDirectory": "./coverage/"
  }
}
